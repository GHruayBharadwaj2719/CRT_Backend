package CRT.entity;

import jakarta.persistence.*;
@Entity
@Table(name = "lessons")
public class Lesson {

    @Id
    private String id;

    private String title;
    private String duration;
    private String category;

    @Column(columnDefinition = "varchar(255)")
    private String notes;

    @Column(columnDefinition = "varchar(255)")
    private String problems;
}
